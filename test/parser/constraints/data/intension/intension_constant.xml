<instance format="XCSP3" type="CSP">
  <variables>
    <array id="x" size="[6]"> 0..35 </array>
  </variables>
  <constraints>

    <intension> lt(mul(2,x[2]),4) </intension>

    <intension> ge(sub(x[0],3),x[1]) </intension>

    <intension> eq(div(x[1],4),dist(1,x[3])) </intension>

    <intension> gt(add(x[1],3),mod(6,x[3])) </intension>
    
  </constraints>
</instance>

